{
  "name": "@sanbricdesigns-monorepo/root",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "front:dev": "turbo run dev --filter=@sanbricdesigns-monorepo/front",
    "back:dev": "turbo run start --filter=@sanbricdesigns-monorepo/back",
    "dev": "turbo run dev",
    "build": "turbo run build",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "prepare": "husky install"
  },
  "devDependencies": {
    "husky": "^8.0.1",
    "turbo": "^1.6.2"
  }
}
